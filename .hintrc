{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "axe/name-role-value": "off",
    "x-content-type-options": "error",
    "http-cache": "error",
    "content-type": ["error",
      {
        ".*\\.js": "application/javascript; charset=utf-8"
      }
    ],
    "no-disallowed-headers": [
      "warning",
      {
        "ignore": [
          "Server"
        ],
        "include": [
          "Custom-Header"
        ]
      }
    ],
    "axe/structure": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 123",
    "not firefox <= 123",
    "not ios_saf <= 17.4",
    "not safari <= 17.4"
  ]
}